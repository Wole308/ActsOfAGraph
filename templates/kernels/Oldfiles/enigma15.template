	{%for sw in context['NUMSUBWORKERS_seq']%}	
	buffer_type chunk{{sw}}_size = SRCBUFFER_SIZE;
	getchunksize(&chunk{{sw}}_size, SRCBUFFER_SIZE, travstate, ((workerID * NUMSUBWORKERS) + ({{sw}} * SRCBUFFER_SIZE)));
	{%endfor%}
	
	{%for sw in context['NUMSUBWORKERS_seq']%}
	keyvalue_t sizes{{sw}}[NUM_PARTITIONS];
	#pragma HLS ARRAY_PARTITION variable=sizes{{sw}} complete
	{%endfor%}
	
	KING{{ddrs}}_LOOP1: for(partition_type p=0; p<NUM_PARTITIONS; p++){
	#pragma HLS PIPELINE II=1
		if(operation.type == EXTRACTCAPSULES){
			{%for sw in context['NUMSUBWORKERS_seq']%}
			sizes{{sw}}[p].value = 0;
			{%endfor%}
		} else if(operation.type == PARTITIONKEYVALUES){
			{%for sw in context['NUMSUBWORKERS_seq']%} 
			sizes{{sw}}[p].key = capsule{{sw}}[p].key; 
			sizes{{sw}}[p].value = capsule{{sw}}[p].value; 
			{%endfor%} 
		} else {}
	}
	
	KING{{ddrs}}_MAINLOOP2: for(buffer_type i=0; i<SRCBUFFER_SIZE * VECTOR_SIZE; i++){
	#pragma HLS PIPELINE II=1
		// load 
		{%for sw in context['NUMSUBWORKERS_seq']%}
		keyvalue_t keyvalue{{sw}} = getkeyvalue(sourcebuffer{{sw}}, i, SRCBUFFER_SIZE);
		{%endfor%}
		
		// operate
		{%for sw in context['NUMSUBWORKERS_seq']%}
		partition_type p{{sw}} = getpartition(keyvalue{{sw}}, llopparams.currentLOP, llopparams.upperlimit);
		{%endfor%}
		
		{%for sw in context['NUMSUBWORKERS_seq']%}
		vertex_t loc{{sw}} = keyvalue{{sw}}.key - globalparams.vbegin;
		{%endfor%}
		{%for sw in context['NUMSUBWORKERS_seq']%}
		if(loc{{sw}} >= PADDEDDESTBUFFER_SIZE * VECTOR_SIZE){ loc{{sw}} = 0; } // REMOVEME.
		{%endfor%}
		
		// operate
		if(operation.type == EXTRACTCAPSULES){
			{%for sw in context['NUMSUBWORKERS_seq']%}
			if(WithinValidRange(i, chunk{{sw}}_size * VECTOR_SIZE) == 1){ sizes{{sw}}[p{{sw}}].value += 1; }
			{%endfor%}
		} else if(operation.type == PARTITIONKEYVALUES){
			{%for sw in context['NUMSUBWORKERS_seq']%}
			setkeyvalue(destbuffer{{sw}}, sizes{{sw}}[p{{sw}}].key + sizes{{sw}}[p{{sw}}].value, keyvalue{{sw}}, PADDEDDESTBUFFER_SIZE);
			{%endfor%}
			{%for sw in context['NUMSUBWORKERS_seq']%}
			if(WithinValidRange(i, chunk{{sw}}_size * VECTOR_SIZE) == 1){ sizes{{sw}}[p{{sw}}].value += 1; }
			{%endfor%}
		} else if(operation.type == REDUCEKEYVALUES){
			{%for sw in context['NUMSUBWORKERS_seq']%}
			keyvalue_t vprop{{sw}} = getkeyvalue(destbuffer{{sw}}, loc{{sw}}, PADDEDDESTBUFFER_SIZE);
			{%endfor%}
			{%for sw in context['NUMSUBWORKERS_seq']%}
			value_t temp{{sw}} = reducefunc(vprop{{sw}}.key, vprop{{sw}}.value, keyvalue{{sw}}.value, globalparams.GraphIter);
			{%endfor%}
			{%for sw in context['NUMSUBWORKERS_seq']%}
			vprop{{sw}}.value = temp{{sw}};
			{%endfor%}
			{%for sw in context['NUMSUBWORKERS_seq']%}
			setkeyvalue(destbuffer{{sw}}, loc{{sw}}, vprop{{sw}}, PADDEDDESTBUFFER_SIZE);	
			{%endfor%}
		} else {}
		
		// stats
		#ifdef _DEBUGMODE_STATS
		if(operation.type == EXTRACTCAPSULES){
			{%for sw in context['NUMSUBWORKERS_seq']%}
			if(WithinValidRange(i, chunk{{sw}}_size * VECTOR_SIZE) == 1){ globalvar_countkvsread4statscollection(1); }
			{%endfor%}
		} else if(operation.type == PARTITIONKEYVALUES){
			{%for sw in context['NUMSUBWORKERS_seq']%}
			if(WithinValidRange(i, chunk{{sw}}_size * VECTOR_SIZE) == 1){ globalstats_countkvspartitioned(1); }
			{%endfor%}
		} else if(operation.type == REDUCEKEYVALUES){
			globalstats_countkvsreduced(NUMSUBWORKERS);
		} else {}
		#endif
	}
	
	#ifdef _DEBUGMODE_CHECKSM
	if(operation.type == EXTRACTCAPSULES){ debuggerA(Debugger_Sizes1, sizes0, 0); } // Debugger
	if(operation.type == PARTITIONKEYVALUES){ debuggerA(Debugger_Sizes2, sizes0, 1); } // Debugger
	#endif 
	KING{{ddrs}}_LOOP3: for(partition_type p=0; p<NUM_PARTITIONS; p++){
	#pragma HLS PIPELINE II=1
		if(operation.type == EXTRACTCAPSULES){
			{%for sw in context['NUMSUBWORKERS_seq']%}
			capsule{{sw}}[p].key = 0;
			capsule{{sw}}[p].value += sizes{{sw}}[p].value;
			{%endfor%}
		} else if(operation.type == PARTITIONKEYVALUES){
			{%for sw in context['NUMSUBWORKERS_seq']%}
			capsule{{sw}}[p].value = sizes{{sw}}[p].value;
			{%endfor%}
		} else { }
	}
	return;
}
{%endfor%}
{%endfor%}
