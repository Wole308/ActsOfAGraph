	if((currentLOP % 2) == 1){ llopparams->baseaddr_sourcekvs_kvs = BASEOFFSET_KVDRAM_KVS; llopparams->baseaddr_destkvs_kvs = BASEOFFSET_KVDRAMWORKSPACE_KVS; } 
