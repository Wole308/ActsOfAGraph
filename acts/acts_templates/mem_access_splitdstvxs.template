			{%for v in context['VECTOR2_SIZE_seq']%}
			vsandmsdata[{{v}}] = {%if(context['XWARE']=="SW")%}mem_convert_and_accessobj->{%endif%}MEMCA_CREATEVBUFFERSTRUCT(vandmdataM[{{v}}], vandmdataN[{{v}}]);
			{%endfor%}	
