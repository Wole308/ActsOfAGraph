	export LD_LIBRARY_PATH="/sw/centos/cuda/10.1/extras/CUPTI/lib64:$LD_LIBRARY_PATH"
	export PATH="/sw/centos/cuda/10.1/bin:/sw/centos/cuda/10.1/NsightCompute-1.0:$PATH"
fi

